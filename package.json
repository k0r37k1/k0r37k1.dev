{
  "name": "k0r37k1.dev",
  "type": "module",
  "version": "1.0.0",
  "description": "Terminal-style portfolio with retro aesthetic - Built with Astro 5, Vue 3, and Tailwind CSS v4",
  "author": "k0r37k1 <https://k0r37k1.dev>",
  "homepage": "https://k0r37k1.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/k0r37k1/k0r37k1.dev.git"
  },
  "keywords": [
    "portfolio",
    "terminal",
    "retro",
    "astro",
    "vue3",
    "tailwindcss",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && node scripts/cleanup-assets.js",
    "preview": "astro preview",
    "astro": "astro",
    "optimize:images": "node scripts/optimize-images.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\"",
    "lint:css": "stylelint \"**/*.{css,astro,vue}\"",
    "lint:css:fix": "stylelint \"**/*.{css,astro,vue}\" --fix",
    "lint:all": "npm run lint && npm run lint:md && npm run lint:css && npm run spell",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts --project=chromium",
    "test:a11y:all": "playwright test tests/e2e/accessibility.spec.ts",
    "test:a11y:advanced": "playwright test tests/e2e/accessibility-advanced.spec.ts --project=chromium",
    "test:a11y:full": "playwright test tests/e2e/accessibility*.spec.ts --project=chromium",
    "test:a11y:ci": "playwright test tests/e2e/accessibility*.spec.ts --reporter=json",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md,astro,vue}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,md,astro,vue}\"",
    "spell": "cspell lint \"**/*.{ts,js,vue,astro,md,json}\" --no-progress --show-suggestions",
    "spell:check": "cspell \"**\"",
    "spell:fix": "cspell lint \"**/*.{ts,js,vue,astro,md}\" --no-progress --show-suggestions --show-context",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.7.0",
    "@astrojs/vue": "^5.1.4",
    "@fontsource/ibm-plex-mono": "^5.2.7",
    "@fontsource/vt323": "^5.2.7",
    "@iconify-json/tabler": "^1.2.26",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/figlet": "^1.7.0",
    "astro": "^5.17.1",
    "axe-core": "^4.11.1",
    "figlet": "^1.10.0",
    "motion-plus-vue": "^1.6.0",
    "motion-v": "^1.10.2",
    "shiki": "^3.22.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cspell/dict-de-de": "^4.1.2",
    "@cspell/dict-fullstack": "^3.2.8",
    "@playwright/test": "^1.58.1",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "cspell": "^9.6.3",
    "glob": "^13.0.1",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lightningcss": "^1.31.1",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "oxlint": "^1.43.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rolldown-vite": "^7.3.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "stylelint": "^17.1.1",
    "stylelint-config-astro": "^2.0.0",
    "stylelint-config-standard": "^40.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0"
  }
}
